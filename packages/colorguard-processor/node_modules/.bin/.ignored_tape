#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/css-colorguard-upgraded/node_modules/.pnpm/tape@5.8.1/node_modules/tape/bin/node_modules:/workspaces/css-colorguard-upgraded/node_modules/.pnpm/tape@5.8.1/node_modules/tape/node_modules:/workspaces/css-colorguard-upgraded/node_modules/.pnpm/tape@5.8.1/node_modules:/workspaces/css-colorguard-upgraded/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/css-colorguard-upgraded/node_modules/.pnpm/tape@5.8.1/node_modules/tape/bin/node_modules:/workspaces/css-colorguard-upgraded/node_modules/.pnpm/tape@5.8.1/node_modules/tape/node_modules:/workspaces/css-colorguard-upgraded/node_modules/.pnpm/tape@5.8.1/node_modules:/workspaces/css-colorguard-upgraded/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tape/bin/tape" "$@"
else
  exec node  "$basedir/../tape/bin/tape" "$@"
fi
