{
  "name": "colorguard-processor",
  "version": "2.0.90",
  "description": "PostCSS processor to keep a watchful eye on your css colors",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "tape test/*.ts | tap-spec"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bryanjtc/css-colorguard-upgraded.git"
  },
  "keywords": [
    "css",
    "colors",
    "lint",
    "csslint",
    "postcss",
    "colorguard",
    "postcss-processor"
  ],
  "author": "Bryan Thomas <49354825+bryanjtc@users.noreply.github.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bryanjtc/css-colorguard-upgraded/issues"
  },
  "homepage": "https://github.com/bryanjtc/css-colorguard-upgraded/tree/master/packages/colorguard-processor/README.md",
  "devDependencies": {
    "@types/node": "20.16.2",
    "@types/postcss-reporter": "7.0.5",
    "@types/tape": "5.6.4",
    "@types/text-table": "0.2.5",
    "colorette": "2.0.20",
    "gitmojis": "3.14.0",
    "log-symbols": "7.0.0",
    "plur": "5.1.0",
    "postcss": "8.4.41",
    "postcss-colorguard": "workspace:*",
    "postcss-reporter": "7.1.0",
    "tap-spec": "5.0.0",
    "tape": "5.8.1",
    "text-table": "0.2.0",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9"
}
